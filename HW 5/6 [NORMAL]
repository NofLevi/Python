def flattenLst6(mat):
    newmat = []
    for sublist in mat:
        for planet in sublist:
            if len(planet) < 6:
                newmat.append(planet)
    return newmat

planets = [['Mercury','Venus','Earth'],['Mars','Jupiter','Saturn'],['Uranus','Nepture','Pluto']]
print(flattenLst6(planets))
